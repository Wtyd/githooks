{
  "name": "githooks/githooks",
  "description": "Ejecuta herramientas de validación y análisis de código cuando suceden eventos de git",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
        "name": "Jorge Lilao",
        "email": "jorgelilaoc@gmail.com"
    },
    {
      "name": "Pablo Lilao",
      "email": "pablolilao@protonmail.com"
    }
],
  "autoload": {
    "psr-4": {
      "GitHooks\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
     }
  },
  "config": {
    "platform": {
        "php": "7.3.0"
    },
    "sort-packages": true
  },
  "bin": ["bin/githooks"],
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "mockery/mockery": "^1.3",
    "phpunit/php-code-coverage": "^7.0.7",
    "phpunit/phpunit": "^8.5"
  },
  "require": {
    "illuminate/console": "^6.0",
    "illuminate/container": "^6.8",
    "illuminate/events": "^6.0",
    "symfony/yaml": "^4.4 || ^5.0"
  }
}
